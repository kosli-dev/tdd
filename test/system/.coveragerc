# This .coveragerc file is named in the environment-variable
# COVERAGE_PROCESS_START=/${XY_CONTAINER_DIR}/test/system/.coveragerc
# set in env_vars/test_system.env

[run]
data_file=/tmp/coverage/system/.coverage
source=/${XY_CONTAINER_DIR}/server
parallel=True
branch=True
omit =
  server/sitecustomize.py
  server/gunicorn_config.py
  server/gunicorn_coverage.py
